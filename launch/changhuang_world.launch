<launch>
  <env name="GAZEBO_MODEL_PATH" value="$(find neuronbot2_gazebo)/models"/>
  <arg name="x_pos" default="-3.0"/>
  <arg name="y_pos" default="1.0"/>
  <arg name="z_pos" default="0.0"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find neuronbot2_multibot)/worlds/changhuang_world.model"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
  <include file="$(find neuronbot2_gazebo)/launch/robot.launch">
    <arg name="namespace" value="/"/>
    <arg name="x_pos" value=" 0.1"/>
    <arg name="y_pos" value=" 0.1"/>
    <arg name="z_pos" value=" 0.0"/>
    <arg name="yaw" value=" 1.57"/>
  </include>
  <include file="$(find neuronbot2_gazebo)/launch/robot.launch">
    <arg name="namespace" value="nb2_1"/>
    <arg name="x_pos" value=" 0.1"/>
    <arg name="y_pos" value=" 3.85"/>
    <arg name="z_pos" value=" 0.0"/>
    <arg name="yaw" value=" 1.57"/>
  </include>
  <include file="$(find neuronbot2_gazebo)/launch/robot.launch">
    <arg name="namespace" value="nb2_2"/>
    <arg name="x_pos" value=" 0.1"/>
    <arg name="y_pos" value="-3.65"/>
    <arg name="z_pos" value=" 0.0"/>
    <arg name="yaw" value=" 1.57"/>
  </include>
</launch>